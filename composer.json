{
	"name": "linawolf/form_double_opt_in",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0",
	"description": "Double Opt-In for the TYPO3 CMS Form Framework",
  "version": "1.0.0",
	"authors": [
    {
      "name": "Lina Wolf",
      "email": "112@llinawolf.de"
    },
    {
      "name": "The TYPO3 Community",
      "role": "Contributor",
      "homepage": "https://typo3.org/community/"
    }
	],
	"require": {
		"ext-json": "*",
    "ext-openssl": "*",
		"typo3/cms-core": "^12.4",
    "typo3/cms-form": "^12.4",
		"typo3/cms-scheduler": "^12.4"
  },
	"require-dev": {
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.2",
		"codeception/module-cli": "^1.1",
		"codeception/module-webdriver": "^1.1",
		"phpstan/phpstan": "^1.8.2",
		"ssch/typo3-rector": "^1.2",
		"typo3/cms-core": "^12.4",
		"typo3/cms-frontend": "^12.4",
		"typo3/cms-install": "^12.4",
		"typo3/coding-standards": "^0.5.0",
		"typo3/tailor": "^1.2",
		"typo3/testing-framework": "^6.14.0"
	},
	"autoload": {
		"psr-4": {
			"LinaWolf\\FormDoubleOptIn\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"LinaWolf\\FormDoubleOptIn\\Tests\\": "Tests"
		}
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"web-dir": ".Build/Web",
			"extension-key": "form_double_opt_in"
		}
	},
	"config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "preferred-install": {
      "typo3/cms-core": "source"
    },
    "sort-packages": true,
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	}
}
